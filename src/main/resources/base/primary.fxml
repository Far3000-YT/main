<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="362.0" prefWidth="330.0" spacing="20.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="base.PrimaryController">
   <children>
      <MenuBar prefHeight="35.0" prefWidth="124.0">
        <menus>
          <Menu mnemonicParsing="false" text="File">
            <items>
              <MenuItem fx:id="menuLogout" mnemonicParsing="false" text="Logout" />
                  <MenuItem fx:id="menuExit" mnemonicParsing="false" text="Exit" />
            </items>
          </Menu>
          <Menu mnemonicParsing="false" text="View">
            <items>
              <MenuItem fx:id="menuProjects" mnemonicParsing="false" text="Projects" />
                  <MenuItem fx:id="menumenuProjects" mnemonicParsing="false" text="Employee" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
      <BorderPane prefHeight="200.0" prefWidth="200.0">
         <left>
            <VBox prefHeight="200.0" prefWidth="75.0" BorderPane.alignment="CENTER">
               <children>
                  <Button mnemonicParsing="false" prefHeight="31.0" prefWidth="126.0" text="Create Project" />
                  <Label text="Project 1" />
               </children>
            </VBox>
         </left>
         <center>
            <VBox prefHeight="136.0" prefWidth="215.0" BorderPane.alignment="CENTER">
               <children>
                  <Label text="Welcome to the Dashboard" />
               </children>
            </VBox>
         </center>
      </BorderPane>
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>